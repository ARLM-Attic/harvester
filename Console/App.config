<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  
  <appSettings>
    <add key="ConsoleColor.Fatal.Background" value="Red"/>
    <add key="ConsoleColor.Fatal.Foreground" value="White"/>
    <add key="ConsoleColor.Error.Background" value="Red"/>
    <add key="ConsoleColor.Error.Foreground" value="Black"/>
    <add key="ConsoleColor.Warn.Background" value="Yellow"/>
    <add key="ConsoleColor.Warn.Foreground" value="Black"/>
    <add key="ConsoleColor.Info.Background" value="White"/>
    <add key="ConsoleColor.Info.Foreground" value="Black"/>
    <add key="ConsoleColor.Debug.Background" value="DarkGray"/>
    <add key="ConsoleColor.Debug.Foreground" value="Black"/>
    <add key="ConsoleColor.Trace.Background" value="DarkGreen"/>
    <add key="ConsoleColor.Trace.Foreground" value="Black"/>
  </appSettings>
  
  <system.diagnostics>
    <trace>
      <listeners>
        <clear />
      </listeners>
    </trace>
  </system.diagnostics>
  
</configuration>